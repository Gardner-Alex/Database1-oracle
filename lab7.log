SQL> COLUMN item_id  FORMAT 9999 HEADING "ITEM|ID"
SQL> COLUMN active_flag FORMAT A6 HEADING "ACTIVE|FLAG"
SQL> COLUMN price_type	 FORMAT 9999 HEADING "PRICE|TYPE"
SQL> COLUMN start_date	FORMAT A20 HEADING "START|DATE"
SQL> COLUMN end_date FORMAT A20 HEADING "END|DATE"
SQL> COLUMN amount    FORMAT 9999 HEADING "AMOUNT"
SQL> 
SQL> SELECT i.item_id
  2  ,	    af.active_flag
  3  ,	    cl.common_lookup_id as price_type
  4  ,CASE
  5  WHEN ((TRUNC(SYSDATE) - TRUNC(i.release_date)) < 31
  6  OR  active_flag = 'N')
  7  THEN i.release_date
  8  ELSE i.release_date + 31
  9  END as start_date
 10  ,Case
 11  WHEN active_flag = 'N'
 12  THEN i.release_date + 30
 13  ELSE
 14  NULL
 15  END as end_date
 16  ,(CASE
 17  	  WHEN (af.active_flag = 'Y' AND (SYSDATE - i.release_date) < 31)
 18  	  OR   (af.active_flag = 'N' AND (SYSDATE - i.release_date) > 30)
 19  	     THEN
 20  		  (CASE
 21  		     WHEN dr.rental_days = '1'
 22  			  THEN 3
 23  		     WHEN dr.rental_days = '3'
 24  			  THEN 10
 25  		     WHEN dr.rental_days = '5'
 26  			  THEN 15
 27  		    END)
 28  	   ELSE
 29  	       TO_NUMBER(dr.rental_days)
 30  	 END) as amount
 31  FROM     item i CROSS JOIN
 32  	     (SELECT 'Y' AS active_flag FROM dual
 33  	      UNION ALL
 34  	      SELECT 'N' AS active_flag FROM dual) af CROSS JOIN
 35  	     (SELECT '1' AS rental_days FROM dual
 36  	      UNION ALL
 37  	      SELECT '3' AS rental_days FROM dual
 38  	      UNION ALL
 39  	      SELECT '5' AS rental_days FROM dual) dr INNER JOIN
 40  	      common_lookup cl ON dr.rental_days = SUBSTR(cl.common_lookup_type,1,1)
 41  WHERE    cl.common_lookup_table = 'RENTAL_ITEM'
 42  AND      cl.common_lookup_column = 'RENTAL_ITEM_TYPE'
 43  AND NOT (active_flag = 'N' AND SYSDATE - i.release_date <= 30)
 44  ORDER BY 1, 2, 3;

